    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "prettier": "^3.3.3",
    "sass": "^1.79.3",
    "stylelint": "^16.9.0",
